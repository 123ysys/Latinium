func drawCircle(r: int){
    for(y:int = -r; y <=r; y = y + 1){
        for(x:int = -r; x <=r; x = x + 1){
            if(x*x + y*y <= r*r){
                print("*")
            }else{
                print(" ")
            }
        }
        print("\n")
    }
}

func main() {
    drawCircle(10)
}