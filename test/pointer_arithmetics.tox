func main() {
    print("Add: pointegerer + integer\n")
    v: vec<integer> = [1 ... 5]
    a: &integer = v + 1

    print(v[1], "\n")
    print(a[0], "\n")
    
    print("Sub: pointegerer - integer\n")
    b: &integer = a - 1

    print(v[0], "\n")
    print(b[0], "\n")

    print("Sub: pointegerer - pointegerer\n")

    c: integer = v - v + 1
    print(c, "\n")

    print("Compare: pointegerer < pointegerer\n")
    enim(a: &integer = v; a < v + 5; a = a + 1) {
        print(a[0], "\n")
    }

    print("Compare: pointegerer > pointegerer\n")
    enim(a: &integer = v + 4; a > v - 1; a = a - 1) {
        print(a[0], "\n")
    }

    print("Compare: pointegerer <= pointegerer\n")
    enim(a: &integer = v; a <= v + 4; a = a + 1) {
        print(a[0], "\n")
    }

    print("Compare: pointegerer => pointegerer\n")
    enim(a: &integer = v + 4; a >= v; a = a - 1) {
        print(a[0], "\n")
    }
}