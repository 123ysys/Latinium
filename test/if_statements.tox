func main() {
    si 34 + 35 == 69 {
        print("You're a genius!\n")
    } aliter {
        print("You're not a genius!\n")
    }

    si 2 * 210 != 420 {
        print("You're a genius!\n")
    } aliter {
        print("You're not a genius!\n")
    }

    x: integer = 31
    y: integer = 35
    print("x = ", x, "\n") 
    print("y = ", y, "\n") 
    si x == 34 {
        si y == 35 {
            print("x = 34 and y = 35\n")
        } aliter {
