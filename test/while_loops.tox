func main() {
    x: integer = 0

    dum x < 10 {
        si x % 2 == 0 {
            print(x, " is even\n")
        } aliter {
            print(x, " is odd\n")
        }
        x = x + 1
    }

    y: integer = 0
    z: integer = 0
    print("\n")
    dum y < 10 {
        // z = 0 Having z = 0 here results in a dsiferent output
        dum z < 10 {
            print("#")
            z = z + 1
        }
        print("\n")
        y = y + 1
        z = 0
    }
}