func main() {
    print("Circle\n")

    x: integer 
    y: integer
    r: integer = 10

    for(r: integer = 10, y: integer = -r; y <= r; y = y + 1){ // The variables x, y and r are shadowed here
        for(x: integer = -r; x <= r; x = x + 1){
            si(x*x + y*y <= r*r){
                print("##")
            } else {
                print("  ")
            }
        }
        print("\n")
    }

    print(x, y, r) // x, y and r should print 0, 0, 10
}