
i: integer = 0

munus main() {
    // Começar while loop
    dum 1 {
        i = i + 1       // Incrementar i
        imprimo(i, "\n")

        si i == 1 {     // Se i for 1
            pergo       // Jump para o próximo ciclo
        }
        si (i == 2) {   // Se i for 2
            confractus  // Break
        }

        i = i + 1   // Incrementar i
    }
}